def abc():

    print "abc"
    print "abc"

    valid_input = False
    while not valid_input:

        print ""
######
        abc = int(raw_input(">"))
######
        print ""

        if abc >= 0 and abc < 0:

            abc()
            valid_input = True

        elif abc >= 0 and abc < 0:

            abc()
            valid_input = True

        else:

            print "abc"

        print ""

